{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "airbnb-base",
      "airbnb-typescript/base",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "no-debugger": "off",
      "no-console": 0,
      "unicorn/prevent-abbreviations": "off",
      "prettier/prettier": "error"
    },
    "settings": {
      "import/extensions": [".js", ".ts"]
    }
}